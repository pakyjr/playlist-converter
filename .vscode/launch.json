  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Utils",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "args": [
        "${workspaceFolder}/packages/utils/lib/utils/test.js"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "env": {
        "STAGE": "dev",
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "API LOCAL",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "args": [
        "${workspaceFolder}/packages/api/lib/api/src/index.js"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "env": {
        "STAGE": "dev",
        "PORT": "8080",
        "SPOTIFY_CLIENT_ID": "0051c80c3951437ea07b02a123abd692",
        "SPOTIFY_REDIRECT_URI": "http://localhost:8080/v1/users/spotify/callback",
        "SPOTIFY_TOKEN_URL": "https://accounts.spotify.com/api/token",
        "SPOTIFY_SECRET": "5c29bb689fb04dffa9cc96ba6cb578bf"
      },
    },
    {
      "type": "node",
      "request": "launch",
      "name": "API TEST",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "args": [
        "${workspaceFolder}/packages/api/lib/index.js"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "env": {
        "STAGE": "dev",
        "PORT": "8080"
      }
    }
  ]
